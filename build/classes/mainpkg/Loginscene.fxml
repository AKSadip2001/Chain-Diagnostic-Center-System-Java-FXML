      <TextField fx:id="useridtextfield" layoutX="218.0" layoutY="110.0" />
      <Label layoutX="149.0" layoutY="114.0" text="Enter ID" />
      <Label layoutX="145.0" layoutY="161.0" text="Password" />
      <PasswordField fx:id="userpasswordtextfield" layoutX="218.0" layoutY="157.0" />
      <Button layoutX="465.0" layoutY="310.0" mnemonicParsing="false" onAction="#loginonclick" prefHeight="26.0" prefWidth="61.0" text="Login" />
      <ComboBox fx:id="usercombobox" layoutX="217.0" layoutY="200.0" onAction="#comboxboxupdated" prefHeight="26.0" prefWidth="150.0" promptText="Login As?" />
